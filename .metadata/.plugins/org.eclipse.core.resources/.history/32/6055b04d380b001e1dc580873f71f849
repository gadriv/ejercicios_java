
public class EjemploCondicionales2 {
	
	public static void main(String[] args) {

		System.out.println("Hellow World");

		int edad = 19;
		int cant = 2;

		if (edad >= 18) {
			System.out.println("Usted puede ingresar");
		} else {
			if (cant >= 2) {
				System.out.println("Usted es menor de edad pero al venir acompado " + "usted es permido");
			} else {
				System.out.println("usted no esta permitido a" + " entrar");
			}
		}

	}
}
